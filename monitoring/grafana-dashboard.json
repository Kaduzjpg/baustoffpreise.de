{
  "title": "API Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))" }
      ]
    },
    {
      "type": "graph",
      "title": "DB Query Duration avg",
      "targets": [
        { "expr": "sum(rate(db_query_duration_seconds_sum[5m])) / sum(rate(db_query_duration_seconds_count[5m]))" }
      ]
    },
    {
      "type": "graph",
      "title": "Email Events",
      "targets": [
        { "expr": "sum by (type,status) (rate(email_events_total[5m]))" }
      ]
    }
  ]
}

